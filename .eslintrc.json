{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "eslint:recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "prettier"],
  "rules": {
    "react/prop-types": "off",
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
